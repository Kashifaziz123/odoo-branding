<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">

    <t t-name="web.NotificationAlert" t-inherit="web.NotificationAlert" t-inherit-mode="extension" owl="1">
        <xpath expr="//div" position="replace">
            <div t-if="isNotificationBlocked" class="alert alert-primary text-center">
                Push notifications have been blocked. Go to your browser settings to allow them.
            </div>
        </xpath>
    </t>

    <div t-extend='res_config_edition'>
   	 <t t-jquery="Setting" t-operation="replace">
        </t>
    </div>
</templates>
